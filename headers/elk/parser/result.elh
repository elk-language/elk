##[
	Represents the result of parsing Elk source code.
]##
sealed primitive noinit class ::Std::Elk::Parser::Result
	##[
		Returns the Abstract Syntax Tree produced by the parser.
	]##
	def ast: Elk::AST::ProgramNode; end

	##[
		Returns a list of diagnostics (warnings, errors, info messages)
	]##
	def diagnostics: DiagnosticList; end
end
