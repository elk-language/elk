module Std::Elk::AST
  ##[
    A single alias entry eg. `new_name old_name`
  ]##
  sealed primitive class AliasDeclarationEntry
    include Node

    init(new_name: IdentifierNode, old_name: IdentifierNode, location: FS::Location = loop; end); end
    def new_name: IdentifierNode; end
    def old_name: IdentifierNode; end
    def location: FS::Location; end
  end
end
