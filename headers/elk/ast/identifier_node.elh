module ::Std::Elk::AST
	##[
		All nodes that should be valid identifiers
		should implement this interface.
	]##
	mixin IdentifierNode
		include ExpressionNode,
						PatternNode


		def to_ast_ident_node: IdentifierNode; end

		interface Convertible
			sig to_ast_ident_node: IdentifierNode
		end
	end
end
