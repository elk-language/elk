module Std::Elk::AST
  ##[
    Float64 literal eg. `5.2f64`, `.5f64`, `45e20f64`
  ]##
  sealed primitive class Float64LiteralNode
    include ExpressionNode,
            TypeNode,
            PatternNode

    ##[
			Indicates that the format of a
			float64 node is invalid.
		]##
		class FormatError < Node::FormatError; end

    init(value: String, location: FS::Location = loop; end); end
    def value: String; end
    def location: FS::Location; end
    def to_float64: Float64; end
  end
end
