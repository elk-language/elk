module Std::Elk::AST
  ##[
    Represents a method lookup expression eg. `Foo::bar`, `a::c`
  ]##
  sealed primitive class MethodLookupNode
    include ExpressionNode,
            UsingEntryNode

    init(receiver: ExpressionNode, name: IdentifierNode, location: FS::Location = loop; end); end
    def receiver: ExpressionNode; end
    def name: IdentifierNode; end
    def location: FS::Location; end
  end
end
