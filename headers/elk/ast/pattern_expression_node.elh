module Std::Elk::AST
  ##[
    Represents a pattern expression `type Foo(a: String, b: > 5)`
  ]##
  sealed primitive class PatternExpressionNode
    include ExpressionNode

    init(pattern_node: PatternNode, location: FS::Location = loop; end); end
