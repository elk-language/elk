module ::Std::Elk::AST
	##[
		Represents AST nodes that are valid expressions and patterns.
	]##
	mixin PatternExpressionNode
		include ExpressionNode,
						PatternNode

		def to_ast_pattern_expr_node: PatternExpressionNode; end

		interface Convertible
			sig to_ast_pattern_expr_node: PatternExpressionNode
		end
	end
end
