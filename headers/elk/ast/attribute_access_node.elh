module Std::Elk::AST
  ##[
    Represents attribute access eg. `foo.bar`
  ]##
  sealed primitive class AttributeAccessNode
    include ExpressionNode

    init(receiver: ExpressionNode, attribute_name: IdentifierNode, location: FS::Location = loop; end); end
    def receiver: ExpressionNode; end
    def attribute_name: IdentifierNode; end
    def location: FS::Location; end
  end
end
