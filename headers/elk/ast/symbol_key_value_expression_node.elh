module Std::Elk::AST
  ##[
    Represents a symbol value expression eg. `foo: bar`
  ]##
  sealed primitive class SymbolKeyValueExpressionNode
    include ExpressionNode

    init(key: IdentifierNode, value: ExpressionNode, location: FS::Location = loop; end); end
    def key: IdentifierNode; end
    def value: ExpressionNode; end
    def location: FS::Location; end
  end
end
