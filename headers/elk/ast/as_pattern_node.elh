module Std::Elk::AST
  ##[
    Represents an as pattern eg. `> 5 && < 20 as foo`
  ]##
  sealed primitive class AsPatternNode
    include PatternNode

    init(pattern_node: PatternNode, name: IdentifierNode, location: FS::Location = loop; end); end
    def pattern_node: PatternNode; end
    def name: IdentifierNode; end
    def location: FS::Location; end
  end
end
