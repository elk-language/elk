module Std::Elk::AST
  ##[
    Int8 literal eg. `5i8`, `1_20i8`, `0xffi8`
  ]##
  sealed primitive class Int8LiteralNode
    include LiteralPatternNode,
            TypeNode

    ##[
			Indicates that the format of an
			int8 node is invalid.
		]##
		class FormatError < Node::FormatError; end

    init(value: String, location: FS::Location = loop; end)! FormatError; end
    def value: String; end
    def location: FS::Location; end
    def to_int8: Int8; end
  end
end
