module Std::Elk::AST
  ##[
    Represents a method lookup with as in using declarations
    eg. `Foo::bar as Bar`.
  ]##
  sealed primitive class MethodLookupAsNode
    include ExpressionNode,
            UsingEntryNode

    init(method_lookup: MethodLookupNode, as_name: IdentifierNode, location: FS::Location = loop; end); end
    def method_lookup: MethodLookupNode; end
    def as_name: IdentifierNode; end
    def location: FS::Location; end
  end
end
