module Std::Elk::AST
  ##[
    BigFloat literal eg. `5.2bf`, `.5bf`, `45e20bf`
  ]##
  sealed primitive class BigFloatLiteralNode
    include LiteralPatternNode,
            TypeNode

    ##[
			Indicates that the format of a
			big float node is invalid.
		]##
		class FormatError < Node::FormatError; end

    init(value: String, location: FS::Location = loop; end); end
    def value: String; end
    def location: FS::Location; end
    def to_bigfloat: BigFloat; end
  end
end
