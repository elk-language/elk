module ::Std::Elk::AST
	##[
		All nodes that should be valid constants
		should implement this interface.
	]##
	mixin ConstantNode
		include TypeNode,
						ExpressionNode,
						UsingEntryNode,
						ComplexConstantNode

		def to_ast_const_node: ConstantNode; end

		interface Convertible
			sig to_ast_const_node: ConstantNode
		end
	end
end
