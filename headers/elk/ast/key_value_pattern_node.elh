module Std::Elk::AST
  ##[
    Represents a key value pattern eg. `foo => bar`
  ]##
  sealed primitive class KeyValuePatternNode
    include PatternNode

    init(key: LiteralPatternNode, value: PatternNode, location: FS::Location = loop; end); end
    def key: LiteralPatternNode; end
    def value: PatternNode; end
    def location: FS::Location; end
  end
end
