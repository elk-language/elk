module Std::Elk::AST
  ##[
    Represents a private constant eg. `_Foo`
  ]##
  sealed primitive class PrivateConstantNode
    include ExpressionNode,
            TypeNode,
            PatternNode

    init(value: String, span: String::Span = loop; end); end
    def value: String; end
    def span: String::Span; end
  end
end
