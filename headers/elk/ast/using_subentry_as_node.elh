module Std::Elk::AST
  ##[
    Represents an identifier with as in using declarations
    eg. `foo as bar`.
  ]##
  sealed primitive class UsingSubentryAsNode
    include ExpressionNode,
            UsingSubentryNode

    init(target: IdentifierNode, as_name: IdentifierNode, location: FS::Location = loop; end); end
    def target: IdentifierNode; end
    def as_name: IdentifierNode; end
    def location: FS::Location; end
  end
end
