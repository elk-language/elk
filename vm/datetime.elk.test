				assert! d <=> Date(2025, 10, 15) == 1
			end

			should "return 0 for equal", ->
				d := DateTime(
					year: 2025,
					month: 11,
					day: 10,
				)
				assert! d <=> Date(2025, 11, 10) == 0
			end

			should "return -1 for less", ->
				d := DateTime(
					year: 2025,
					month: 11,
					day: 10,
				)
				assert! d <=> Date(2025, 12, 4) == -1
			end
		end

		context "datetime argument", ->
