					assert! s.months == 0
					assert! s.days == 0
					assert! s.hours == 0
					assert! s.minutes == 0
					assert! s.seconds == -1
					assert! s.milliseconds == -98
					assert! s.microseconds == 0
					assert! s.nanoseconds == 0
				end

				should "create a span with float milliseconds", ->
					s := DateTime::Span.parse("2.25ms")
					assert! s <<: DateTime::Span
					assert! s.to_string == "2ms 250µs"
					assert! s.years == 0
					assert! s.months == 0
					assert! s.days == 0
					assert! s.hours == 0
					assert! s.minutes == 0
					assert! s.seconds == 0
