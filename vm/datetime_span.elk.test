	context "init", ->
		should "create a zero value", ->
			s := DateTime::Span()
			assert! s <<: DateTime::Span
			assert! s.to_string == "0D"
			assert! s.years == 0
			assert! s.months == 0
			assert! s.days == 0
			assert! s.nanoseconds == 0
		end

		should "create with all positional args", ->
			s := DateTime::Span(10, 5, 2, 9, 13, 8, 130, 540, 931)
			assert! s <<: DateTime::Span
			assert! s.to_string == "10Y 5M 2D 9h 13m 8s 130ms 540µs 931ns"
			assert! s.years == 10
			assert! s.months == 5
			assert! s.days == 2
			assert! s.hours == 9
			assert! s.minutes == 13
			assert! s.seconds == 8
			assert! s.milliseconds == 130
			assert! s.microseconds == 540
			assert! s.nanoseconds == 931
		end

		should "create with a few positional args", ->
			s := DateTime::Span(10, 5)
			assert! s <<: DateTime::Span
			assert! s.to_string == "10Y 5M"
			assert! s.years == 10
			assert! s.months == 5
			assert! s.days == 0
			assert! s.hours == 0
			assert! s.minutes == 0
			assert! s.seconds == 0
			assert! s.milliseconds == 0
			assert! s.microseconds == 0
			assert! s.nanoseconds == 0
		end


		should "create with named args", ->
			s := DateTime::Span(months: 10, years: 5, nanoseconds: 54)
			assert! s <<: DateTime::Span
			assert! s.to_string == "5Y 10M 54ns"
			assert! s.years == 5
			assert! s.months == 10
			assert! s.days == 0
			assert! s.hours == 0
			assert! s.minutes == 0
			assert! s.seconds == 0
			assert! s.milliseconds == 0
			assert! s.microseconds == 0
			assert! s.nanoseconds == 54
		end
	end

	context "unary minus", ->
		should "negate positive span", ->
			s := DateTime::Span(10, 2, 9, 3, 18)
			r := -s
			assert! r <<: DateTime::Span
			assert! r.to_string == "-10Y -2M -9D -3h -18m"
			assert! r.years == -10
			assert! r.months == -2
			assert! r.days == -9
			assert! r.hours == -3
			assert! r.minutes == -18
		end

		should "negate negative span", ->
			s := DateTime::Span(-10, -2, -9, -3, -18)
			r := -s
			assert! s <<: DateTime::Span
			assert! r.to_string == "10Y 2M 9D 3h 18m"
			assert! r.years == 10
			assert! r.months == 2
			assert! r.days == 9
			assert! r.hours == 3
			assert! r.minutes == 18
		end
	end

	context "unary plus", ->
		should "not change positive span", ->
			s := DateTime::Span(10, 2, 9, 3, 18)
			r := +s
			assert! r <<: DateTime::Span
			assert! r.to_string == "10Y 2M 9D 3h 18m"
			assert! r.years == 10
			assert! r.months == 2
			assert! r.days == 9
			assert! r.hours == 3
			assert! r.minutes == 18
		end

		should "not change negative span", ->
			s := DateTime::Span(-10, -2, -9, -3, -18)
			r := +s
			assert! r <<: DateTime::Span
			assert! r.to_string == "-10Y -2M -9D -3h -18m"
			assert! r.years == -10
			assert! r.months == -2
			assert! r.days == -9
			assert! r.hours == -3
			assert! r.minutes == -18
		end
	end

	context "+", ->
		context "duration argument", ->
			should "add date span", ->
				s := DateTime::Span(10, 2, 9, 3, 18)
				var d: Duration = Date::Span(1, 2, 3)
				r := s + d
				assert! r <<: DateTime::Span
				assert! r.to_string == "11Y 4M 12D 3h 18m"
				assert! r.years == 11
				assert! r.months == 4
				assert! r.days == 12
				assert! r.hours == 3
				assert! r.minutes == 18
			end

			should "add time span", ->
				s := DateTime::Span(10, 2, 9, 3, 18)
				var d: Duration = Time::Span(1, 2, 3)
				r := s + d
				assert! r <<: DateTime::Span
				assert! r.to_string == "10Y 2M 9D 4h 20m 3s"
				assert! r.years == 10
				assert! r.months == 2
				assert! r.days == 9
				assert! r.hours == 4
				assert! r.minutes == 20
				assert! r.seconds == 3
			end

			should "add datetime span", ->
				s := DateTime::Span(10, 2, 9, 3, 18)
				var d: Duration = DateTime::Span(1, 2, 3, 4, 5, 6)
				r := s + d
				assert! r <<: DateTime::Span
				assert! r.to_string == "11Y 4M 12D 7h 23m 6s"
				assert! r.years == 11
				assert! r.months == 4
				assert! r.days == 12
				assert! r.hours == 7
				assert! r.minutes == 23
				assert! r.seconds == 6
			end
		end

		should "add date span", ->
			s := DateTime::Span(10, 2, 9, 3, 18)
			r := s + Date::Span(1, 2, 3)
			assert! r <<: DateTime::Span
			assert! r.to_string == "11Y 4M 12D 3h 18m"
			assert! r.years == 11
			assert! r.months == 4
			assert! r.days == 12
			assert! r.hours == 3
			assert! r.minutes == 18
		end

		should "add time span", ->
			s := DateTime::Span(10, 2, 9, 3, 18)
			r := s + Time::Span(1, 2, 3)
			assert! r <<: DateTime::Span
			assert! r.to_string == "10Y 2M 9D 4h 20m 3s"
			assert! r.years == 10
			assert! r.months == 2
			assert! r.days == 9
			assert! r.hours == 4
			assert! r.minutes == 20
			assert! r.seconds == 3
		end

		should "add datetime span", ->
			s := DateTime::Span(10, 2, 9, 3, 18)
			r := s + DateTime::Span(1, 2, 3, 4, 5, 6)
			assert! r <<: DateTime::Span
			assert! r.to_string == "11Y 4M 12D 7h 23m 6s"
			assert! r.years == 11
			assert! r.months == 4
			assert! r.days == 12
			assert! r.hours == 7
			assert! r.minutes == 23
			assert! r.seconds == 6
		end
