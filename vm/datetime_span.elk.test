					assert! s.months == -1
					assert! s.days == 0
					assert! s.hours == 0
					assert! s.minutes == 0
					assert! s.seconds == 0
					assert! s.milliseconds == 0
					assert! s.microseconds == 0
					assert! s.nanoseconds == 0
